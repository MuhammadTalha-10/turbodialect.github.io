root ::= ports identifier ports "\n"

# Ports
ports ::= "(" port+ ")"
port ::= ((identifier | arrayDecl) ":" naturalNum) ("," (identifier | arrayDecl) ":" naturalNum)*

# Array
arrayDecl ::= identifier ("[" naturalNum "]")+

# Primitives
identifier ::= [a-zA-Z] [a-zA-Z0-9]*
naturalNum ::= [1-9] [0-9]*

